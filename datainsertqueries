CREATE TABLE DESCRIPTION(
   REF_LIB_SSN INTEGER  NOT NULL 
  ,AVAILABLE_IN_LIB_ISBN VARCHAR(23) NOT NULL,
FOREIGN KEY(REF_LIB_SSN) REFERENCES STAFF_MEMBER(STAFF_SSN),
   FOREIGN KEY(AVAILABLE_IN_LIB_ISBN) REFERENCES BOOKINLIB(ISBN)
);
