CREATE TABLE ISSUE_CARD(
   SSN    INTEGER  NOT NULL PRIMARY KEY 
  ,Date_of_expiry VARCHAR(12)  NOT NULL
  ,STAFF_SSN INTEGER  NOT NULL,
FOREIGN KEY(SSN) REFERENCES LIBRARY_MEMBER(SSN),
    FOREIGN KEY(STAFF_SSN) REFERENCES STAFF_MEMBER(STAFF_SSN)
);
