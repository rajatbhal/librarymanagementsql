CREATE TABLE BOOKINLIB(
   ISBN    VARCHAR(23) NOT NULL PRIMARY KEY
  ,Title   VARCHAR(101) NOT NULL
  ,Binding_type VARCHAR(9) NOT NULL
  ,Language VARCHAR(7) NOT NULL
  ,Author  VARCHAR(36)
  ,Subject VARCHAR(19) NOT NULL
  ,Available VARCHAR(3) NOT NULL
  ,Can_be_lent VARCHAR(3) NOT NULL
  ,No_of_copies INTEGER  NOT NULL
  ,Lented_books INTEGER  NOT NULL
  ,Book_descp VARCHAR(487)
);
