CREATE TABLE ISSUE_BOOK(
   STAFF_SSN INTEGER  NOT NULL PRIMARY KEY
  ,SSN    INTEGER  NOT NULL
  ,ISBN   VARCHAR(31) NOT NULL
  ,Issue_date VARCHAR(12)  NOT NULL
  ,Date_of_reminder VARCHAR(12) NOT NULL
  ,Due_date VARCHAR(12) NOT NULL
  ,Grace_period INT  NOT NULL
, 
 
  constraint FK_STAFF_SSN FOREIGN KEY(STAFF_SSN) REFERENCES STAFF_MEMBER(STAFF_SSN),
    FOREIGN KEY(SSN) REFERENCES LIBRARY_MEMBER(SSN),
    FOREIGN KEY(ISBN) REFERENCES BOOKINLIB(ISBN)
);
